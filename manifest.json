{
  "name": "Oborobot",
  "version": "0.0.1",
  "description": "This is an oborobot-chrome-extention.",
  "permissions": ["activeTab","declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
